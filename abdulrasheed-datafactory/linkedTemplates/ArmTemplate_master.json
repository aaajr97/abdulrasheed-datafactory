{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "abdulrasheed-datafactory"
		},
		"sacompanyabdulrasheed_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sacompanyabdulrasheed'"
		},
		"abdulrasheedsynapse_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'abdulrasheedsynapse'"
		},
		"sacompanyabdulrasheed_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sacompanyabdulrasheed.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "abdulrasheed-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"sacompanyabdulrasheed_accountKey": {
						"value": "[parameters('sacompanyabdulrasheed_accountKey')]"
					},
					"abdulrasheedsynapse_connectionString": {
						"value": "[parameters('abdulrasheedsynapse_connectionString')]"
					},
					"sacompanyabdulrasheed_properties_typeProperties_url": {
						"value": "[parameters('sacompanyabdulrasheed_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}